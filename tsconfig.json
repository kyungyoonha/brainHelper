{
  "compilerOptions": {
    "target": "es5",
    "downlevelIteration": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@hooks/*": ["./src/hooks/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@domain/*": ["./src/domain/*"],
      "@utils/*": ["./src/utils/*"],
      "@context/*": ["./src/context/*"],
      "@components": ["./src/components"],
      "@interfaces*": ["./src/interfaces"],
      "@styles*": ["./src/styles"],
      "@service": ["./src/service"],
      "@config": ["./src/config"],
      "@typings/http/*": ["./src/interfaces/typings/kr/woobok/protocol/http/*"],
      "@typings/model/*": [
        "./src/interfaces/typings/kr/woobok/protocol/model/*"
      ]
    },
    // "noUnusedLocals": true,
    // "noUnusedParameters": true,
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
